{
  "name": "allergenius",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Allergenius/allergenius.git"
  },
  "author": "Erin Clancy, Kayla Himmelberger, Sarah Lopriore, Nicholette Sams",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bs-custom-file-input": "^1.3.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^7.4.2",
    "jwt-decode": "^2.2.0",
    "kyanite": "^0.10.3",
    "moment": "^2.23.0",
    "mysql": "^2.14.1",
    "mysql2": "^1.6.1",
    "proptypes": "^1.1.0",
    "react": "^16.7.0",
    "react-datepicker": "^2.0.0",
    "react-datetime-picker": "^2.1.1",
    "react-router-dom": "^4.3.1",
    "react-widgets": "^4.4.9",
    "react-widgets-moment": "^4.0.25",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.9"
  },
  "bugs": {
    "url": "https://github.com/Allergenius/allergenius/issues"
  },
  "engines": {
    "node": "10.3.0"
  }
}
